/*
 * @param h 該Cell對應之表頭
 * @param v 該Cell對應之值
 * @param o 最終輸出資料
 **/
function core(h, v, o) {
	if(!!h &amp;&amp; h.indexOf(&quot;\.&quot;) == -1) {
      // as &quot;many&quot; in assocciation
      if(h.indexOf(&quot;\/&quot;) != -1) {
        var h1 = h.replace(/((?![\/])[\w]+)\/(.*)/g, &apos;$1&apos;);
        var h01 = h.replace(/((?![\/])[\w]+)\/((?![\/])[\w]+)(.*)/g, &apos;$2&apos;);
        var h2 = h.replace(/((?![\/])[\w]+)\/(.*)/g, &apos;$2&apos;);  
        // 只有這種情況(insureds/0/localName)才組成JSON ARRAY
        if(isNaN(h1) &amp;&amp; !isNaN(h01)) {
          var tp = [];
          if(!!!o[h1]) eval(o[h1] = tp);
          // var h2 = h.replace(/((?![\/])[\w]+)\/(.*)/g, &apos;$2&apos;);  
          core(h2, v, o[h1]);  
        } else {
          var tp = {};
          if(!!!o[h1]) eval(o[h1] = tp);
          // var h2 = h.replace(/((?![\/])[\w]+)\/(.*)/g, &apos;$2&apos;);  
          core(h2, v, o[h1]);            
        }
        
      } 
      
      else {
        if(isNaN(h)) {
          if(!!!o[h1]) eval(o[h] = {});
          var isValueAsDate = /[\d]+T[\d]{2}:[\d]{2}:[\d]{2}/g.test(v);
          // var isValueAsDate = v.indexOf(&apos;T&apos;) == -1;
          var value = isNaN(v) ? (isValueAsDate ? JSON.stringify(v).replace(/\&quot;/g, &apos;&apos;) : new String(v)) : v;
          o[h] = value;
        } else {
          if(!!!o[h1]) eval(o[h] = []);
          eval(o[h].push(new String(v)));   
        }
      }
	}
}



